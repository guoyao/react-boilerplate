{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "react boilerplate project",
  "repository": {
    "type": "git",
    "url": "https://github.com/guoyao/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "guoyao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guoyao/react-boilerplate/issues"
  },
  "homepage": "https://github.com/guoyao/react-boilerplate#readme",
  "engines": {
    "npm": ">=3",
    "node": ">=6"
  },
  "scripts": {
    "build": "npm run clean && npm run bundle",
    "bundle": "babel-node ./node_modules/webpack/bin/webpack --progress --color",
    "start": "babel-node server",
    "clean": "rm -rf dist",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- app server *.js"
  },
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "commander": "^2.15.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "happypack": "^5.0.0-beta.3",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
